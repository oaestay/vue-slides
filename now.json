{
  "version": 2,
  "name": "vue-slides",
  "alias": ["vue-slides.oscarestay.com", "www.vue-slides.oscarestay.com"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "^/media/(.*)", "dest": "/videos/$1" },
    { "src": "^/videos/(.*)", "dest": "/videos/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
